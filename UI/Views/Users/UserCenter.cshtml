@model UI.Models.UsersViewModel
@{
    ViewBag.Title = "学践网-用户个人中心";
}
@section styles{

    <link href="~/Content/Users/UserCenter.css" rel="stylesheet" />
}

<section>
    <div class="leftUserCenter">
        <img src="~/Images/user-head/11.jpg" style="width:100px;" />
        <input type="file" value="修改头像" />
        <button>上传头像</button>
    </div>
    <div class="rightUserCenter">  
            <div class="topbar">
                兼职订单
            </div> 
        <div>

                <table>
                    <tr>
                        <th>订单名称</th>
                        <th>发布日期</th>
                        <th>订单状态</th>
                        <th>预约人数</th>
                    </tr>
                    @foreach (var item in Model.Receive)
                    {
                        <tr>
                            <td><a href="/Activities/Index"> @item.Activities.ActContent</a></td>
                            <td>@item.ReceiveTime</td>
                            @*<td>@item.EndTime</td>*@
                        </tr>
                    }
                </table>
        </div>
    </div>
</section>
